    let allContacts = [...CONTACTS];

    function renderContacts(contactsToRender = allContacts) {
      const container = document.getElementById('contactsContainer');
      const html = contactsToRender.map(c => `
        <a href="tel:${c.number}" class="speed-dial-item" data-name="${escapeHtml(c.name)}" data-number="${c.number}">
          <div class="name">${escapeHtml(c.name)}</div>
          <div class="number">
            <span class="tel-icon"></span>
            ${c.number}
          </div>
          <div class="contact-icon">‚òéÔ∏è</div>
        </a>
      `).join('');
      container.innerHTML = html;
      updateStats(contactsToRender.length);
    }

    function escapeHtml(text) {
      const div = document.createElement('div');
      div.textContent = text;
      return div.innerHTML;
    }

    function updateStats(visibleCount) {
      const statsDisplay = document.getElementById('statsDisplay');
      if (visibleCount === allContacts.length) {
        statsDisplay.textContent = `Showing all ${allContacts.length} contacts`;
      } else {
        statsDisplay.textContent = `Showing ${visibleCount} of ${allContacts.length} contacts`;
      }
    }

    function filterContacts() {
      const searchTerm = document.getElementById('searchInput').value.toLowerCase().trim();
      const noResults = document.getElementById('noResults');
      const container = document.getElementById('contactsContainer');

      if (!searchTerm) {
        renderContacts(allContacts);
        noResults.classList.add('hidden');
        container.classList.remove('hidden');
        return;
      }

      const filtered = allContacts.filter(contact => {
        const name = contact.name.toLowerCase();
        const number = contact.number.toLowerCase();
        return name.includes(searchTerm) || number.includes(searchTerm);
      });

      if (filtered.length === 0) {
        noResults.classList.remove('hidden');
        container.classList.add('hidden');
        updateStats(0);
      } else {
        noResults.classList.add('hidden');
        container.classList.remove('hidden');
        renderContacts(filtered);
      }
    }

    // Initialize
    renderContacts();

    // Add event listener for contact clicks to log
    document.addEventListener('click', function(e) {
      const contactItem = e.target.closest('.speed-dial-item');
      if (contactItem) {
        const name = contactItem.getAttribute('data-name');
        const number = contactItem.getAttribute('data-number');
        console.log('[Tel Protocol] Initiating call to:', name, '(' + number + ')');
      }
    });
  </script>
